{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "jest.autoRun": {
        "onSave": false,
        "onStartup": false
    },
    "jest.jestCommandLine": "yarn run test",
    "jest.debugMode": false,
    "jest.testExplorer": {
        "enabled": true
    },
}